{
  "name": "turtle-draw",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev-*\"",
    "dev-server": "nodemon -w server/src -x tsx server/src/app.ts",
    "dev-build": "nodemon -w rust/src -x wasm-pack build ./rust --dev",
    "start": "NODE_ENV=production node --import tsx server/src/app.ts",
    "build": "wasm-pack build ./rust --release && tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "load_db": "node scripts/loadDatabase.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "^2.2.1",
    "argon2": "^0.40.1",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.2.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "redux-persist": "^6.0.0",
    "rust-scheme": "file:rust/pkg",
    "tsx": "^4.7.1",
    "vite-express": "^0.15.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  }
}
