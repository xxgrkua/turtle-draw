{
  "name": "turtle-draw",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev-*\"",
    "dev-server": "nodemon -w src/server -x node src/server/main.js",
    "dev-build": "nodemon -w rust/src -x wasm-pack build ./rust --dev",
    "start": "NODE_ENV=production node src/server/main.js",
    "build": "wasm-pack build ./rust --release && tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "load_db": "node scripts/loadDatabase.js"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.6.43",
    "@reduxjs/toolkit": "^2.1.0",
    "antd": "^5.12.8",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongoose": "^8.0.4",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "rust-scheme": "file:./rust/pkg",
    "vite-express": "^0.13.0",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
